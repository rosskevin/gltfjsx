{
  "name": "@rosskevin/gltfjsx",
  "version": "7.0.0",
  "description": "GLTF to JSX converter",
  "prettier": "@alienfast/prettier-config",
  "scripts": {
    "build": "tsup-node && chmod +x dist/cli.js",
    "build:ide": "echo 'tsc -b' && tsc -b",
    "lint:fix": "eslint . --cache --fix",
    "test": "vitest",
    "release": "yarn auto shipit"
  },
  "type": "module",
  "bin": "./dist/cli.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "gltf",
    "jsx",
    "react",
    "fiber",
    "three",
    "threejs",
    "webp"
  ],
  "author": "Paul Henschel",
  "maintainers": [
    "Max Rusan"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmndrs/gltfjsx.git"
  },
  "bugs": {
    "url": "https://github.com/pmndrs/gltfjsx/issues"
  },
  "homepage": "https://github.com/pmndrs/gltfjsx#readme",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@gltf-transform/core": "4.1.1",
    "@gltf-transform/extensions": "4.1.1",
    "@gltf-transform/functions": "4.1.1",
    "draco3dgltf": "^1.5.7",
    "is-var-name": "^2.0.0",
    "keyframe-resample": "^0.1.0",
    "meow": "^13.2.0",
    "meshoptimizer": "^0.22.0",
    "node-three-gltf": "2.0.1",
    "prettier": "3.4.2",
    "read-pkg-up": "^11.0.0",
    "sharp": "^0.33.5",
    "three": "0.173.0",
    "three-stdlib": "^2.35.13",
    "ts-morph": "^25.0.1"
  },
  "optionalDependencies": {
    "libvips": "^0.0.2"
  },
  "devDependencies": {
    "@alienfast/eslint-config": "^5.1.25",
    "@alienfast/prettier-config": "^1.0.1",
    "@alienfast/tsconfig": "^1.0.4",
    "@auto-it/all-contributors": "^11.3.0",
    "@auto-it/first-time-contributor": "^11.3.0",
    "@auto-it/released": "^11.3.0",
    "@types/draco3dgltf": "^1.4.3",
    "@types/node": "^22.13.1",
    "@types/three": "^0.173.0",
    "auto": "^11.3.0",
    "chalk": "^5.4.1",
    "eslint": "^9.19.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vitest": "^3.0.5"
  },
  "lint-staged": {
    "*.{json,md,gql}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx,js}": [
      "eslint . --cache --ext .js,.ts,.tsx --fix",
      "git add"
    ]
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/react-three-fiber"
  },
  "packageManager": "yarn@4.6.0",
  "auto": {
    "plugins": [
      "npm",
      "all-contributors",
      "first-time-contributor",
      "released"
    ]
  }
}
